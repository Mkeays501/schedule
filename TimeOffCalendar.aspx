                            return '<span style="margin-right: 0.5rem;" class="icon small ' + data.iconClass + '"></span><span class="icon small ' + data.secondaryIcon.class + '"></span>';
                        } else if (false || data.userIsOwner) {
                            return '<span class="icon small ' + data.iconClass + '"></span>';
                        } else {
                            return '<span class="icon small shared-' + data.eventType + '"></span>';
                        }
                    }
                },
                {
                    data : null,
                    render: function (data) {
                        var renderer = $.fn.dataTable.render.text()
                        return renderer.display(data.header) + '<br/>' + renderer.display(data.subHeader);
                    }
                }
            ],
        };
    }

    function getTorTableOptions() {
        return {
            localization: {"day":"day","days":"days","emptyTable":"There are no time off requests to display"},
            timeFormat: pw.getTimeFormat(2 == 1),
            dateFormat: pikadayOptions.format,
            digitsToDisplay: 2,
