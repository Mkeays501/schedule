    function getDailyTimeOffRequestGroups(groupIds) {
        var torsDeferred = $.Deferred();

        if (!groupIds || !groupIds.length) {
            torsDeferred.resolve([])
        } else {
            retrievingCsrfToken.done(function() {
                return $.ajax({
                    type: 'POST',
                    url: '/pwnextv2api/tom/TimeOffRequestGroups?usePascalCasePropertyNames=true',
                    contentType: 'application/json; charset=utf-8',
                    headers: {
                        'RequestVerificationToken': apiTokenManager.csrfToken
                    },
                    data: JSON.stringify({ groupIds : groupIds }),
                    success: function(result) {
                        torsDeferred.resolve(result);
                    }
                })
            });
        }

        return torsDeferred;
    }

