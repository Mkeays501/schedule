    function hasPendingChildren (torTableRow) {
        var pendingStates = [0, 3];

        for (i = 0; i < torTableRow.ChildRows.length; i++) {
            if (0 <= $.inArray(torTableRow.ChildRows[i].StatusId, pendingStates))
                return true;
        }

        return false;
    }

    function getTorData(dateRange) {
        var data = Object.assign({}, dateRange, {
            statuses: [
                'Pending',
                'Approved',
                'PendingWithdrawal'
            ]
        });

        if (true) {
            return getCalendarEvents('/pwnextv2api/tom/TimeOffRequestGroups', data);
        } else {
            return $.Deferred().resolve([]);
        }
    }

    function getDateRangeIndicators() {
        if ($('#rdoMonthView').is(':checked')) {
            var dateRange = dateRangeSelector.getRange()[1];
            var formattedDates = {
                lowerBound: pw.dates.formatServer(dateRange.lowerBound),
                // Up to but not including midnight of the next day
                upperBound: pw.dates.formatServer(dateRange.upperBound.clone().add(1, 'days')),
                menuId: 1576,
                employeeId: 809
            }

            getCalendarEvents('/pwnextv2/absencemanagement/TimeOffCalendar/dateCard/indicators', formattedDates).done(function (result) {
                dateRangeSelector.addIndicators(result);
            });
        }
    }

