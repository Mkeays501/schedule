    function toggleCalendarViewMode(event) {
        var view = event.target.value;
        var showCalendar = view === '0';

        $('#divCalendar').toggle(showCalendar);
        $('#divAgenda').toggle(!showCalendar);
        toggleCalendarDropDownOptions(showCalendar && calendar.isDataExpandable());
        setPageData(showCalendar)
        saveViewPreferences({CalendarView: view});
    }

    function setPageData(showCalendar) {
        if (showCalendar) setCalendarData();
        if (!showCalendar) setAgendaData();
    }

    function setCalendarData() {
        calendar.data((filteredShifts.concat(filteredTors)).sort($.fn.calendarFilter.sortCalendarItems));
        calendar.redraw();
    }

    function setAgendaData() {
        agenda.data(filteredTors, availableHolidays, filteredShifts);
    }

    function saveViewPreferences(preferences) {
        preferences = $.extend({
            CalendarView: null,
            CalendarDateSelector: null,
            TorAccrualId: null,
            DisplaySharedCalendar: null
        }, preferences);

        return $.ajax({
            type: 'POST',
            url: '/tom/services/TimeOffCalendarService.asmx/saveViewPreferences',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({ viewPreferences: preferences })
        });
    }

    function getDateBounds() {
        if($('[name="calendarRange"]:checked').val() === '1') {
            return customRangeSelector.getDateBounds();
        } else {
            return dateRangeSelector.getDateBounds();
        }
    }

    function getDateBoundsForServer() {
        var bounds = getDateBounds();

        return {
            lowerBound: pw.dates.formatServer(bounds.lowerBound),
            upperBound: pw.dates.formatServer(bounds.upperBound),
        };
    }

    function getGeneratedOnDate() {
        return pw.datetimes.momentToServerDateTime(moment());
    }

    function saveCalendarPdf(event) {
        event.preventDefault();

        var bounds = getDateBoundsForServer();
        var viewMode = $('input[name=calendarView]:checked').val();
        var exportUrl = 'tom/views/CalendarExport.aspx?LowerBound=' + bounds.lowerBound + '&UpperBound=' + bounds.upperBound + '&GeneratedOn=' + getGeneratedOnDate() + '&ViewMode=' + viewMode;
        var fileName = 'Calendar_' + bounds.lowerBound + '_to_' + bounds.upperBound;

        window.open('/pdf/api/urltopdf' +
            '?UrlToConvert=' + encodeURIComponent(exportUrl.replace(' ','_')) +
            '&FileName=' + encodeURIComponent(fileName));
    }

    function printCalendar(event) {
        event.preventDefault();

        var bounds = getDateBoundsForServer();
        var viewMode = $('input[name=calendarView]:checked').val();
        var queryString = 'action=print&LowerBound=' + bounds.lowerBound + '&UpperBound=' + bounds.upperBound + '&GeneratedOn=' + getGeneratedOnDate() + '&ViewMode=' + viewMode;

        window.open('/tom/views/CalendarExport.aspx?' + queryString);
    }

    function emailCalendar(event) {
        event.preventDefault();

        var modal = getEmailModal();
        var validator = modal.validateForm(getModalValidation());

        function getEmailModal () {
            return $('#emailCalendarModal').empty().pwModal('load', {
                headerContent: '<h1>Email Calendar<h1>',
                mainContent: $('<p>')
                                    .append('To email the calendar, enter the recipient address below.')
                                    .append('<input type="email" id="txtEmailAddress" name="emailAddress" />'),
                buttons: {
                    first: $.extend(true, {}, $.fn.pwModal.cancelButton, { name: 'pwModalEmailCancel' }),
                    second: {
                        name: 'pwModalSendButton',
                        class: 'primary',
                        text: 'Send',
                        onClick: sendEmail
                    }
                }
            }).pwModal("show");
        }

        function getModalValidation () {
            return [{
                name: 'emailAddress',
                errorValidators: [{
                    validator: lib.validation.isValidEmail,
                    message: 'Invalid',
                    onlyValidateOnSave: true
                }]
            }];
        }

        function sendEmail(e, $modalData) {
            pw.ui.addAjaxStyles(e.target, validator.validate()).done(function() {
                var parameters = $.extend($modalData.serializeObject(), getDateBoundsForServer(), {
                    generatedOn : getGeneratedOnDate(),
                    viewMode : $('input[name=calendarView]:checked').val()
                });

                pw.ui.addAjaxStyles(e.target, emailCalendarPdf(parameters)).done(function () {
                    $modalData.pwModal("hide");
                });
            });
        }

        function emailCalendarPdf (parameters) {
            return $.ajax({
                type: 'POST',
                url: '/TOM/Services/TimeOffCalendarService.asmx/EmailExportPdf',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(parameters)
            });
        }
    }

    function selectViewPreferences(preferences) {
        var showCalendar = preferences.CalendarView === 0 || !Foundation.utils.is_large_up();
        var showMonthSelector = preferences.CalendarDateSelector === 0;

        $('#rdoCalendar').prop('checked', showCalendar);
        $('#rdoAgenda').prop('checked', !showCalendar);
        $('#divCalendar').toggle(showCalendar);
        $('#divAgenda').toggle(!showCalendar);
        toggleCalendarDropDownOptions(showCalendar && calendar.isDataExpandable());

        $('#rdoMonthView').prop('checked', showMonthSelector);
        $('#rdoCustomView').prop('checked', !showMonthSelector);
        dateRangeSelector.toggle(showMonthSelector);
        customRangeSelector.toggle(!showMonthSelector);
        $('#accSmallRangeSelector').toggleClass('hide-for-small', showMonthSelector);

        
        $('#chkSharedCalendar').prop('checked', preferences.DisplaySharedCalendar);

        toggleEmployeeNameColumn(preferences.DisplaySharedCalendar);
        
    }

    function toggleCalendarDropDownOptions(areRowsExpandable) {
        var isFilterShown = pw.getCurrentBreakpointVal() === pw.BREAKPOINT.SMALL && $('#aFilterTors').length > 0; //This list item is only shown on small screens
        var isNotMediumScreen = pw.getCurrentBreakpointVal() !== pw.BREAKPOINT.MEDIUM;
        var isAdmin = false;

        $('#aExpandRows').toggle(areRowsExpandable);
        $('#optionsHeader').toggle(areRowsExpandable || isFilterShown || isAdmin || true);
        $('#optionsDivider').toggle((areRowsExpandable || isFilterShown || true) && isNotMediumScreen);
    }

