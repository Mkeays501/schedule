                                <div class="columns small-12">
                                    {{ totalHours + ' hrs' }}
                                </div>
                            </div>
                        </div>
                        <div class="columns small-12 medium-4 show-for-medium-up">
                            <span class="icon" :class="breakIcon"></span>
                            <b>{{ shiftbreak.description }}</b>
                        </div>
                        <div class="columns small-12 medium-4 show-for-medium-up">
                            {{ formattedBreakRange }}
                        </div>
                        <div class="columns small-12 medium-4 show-for-medium-up">
                            {{ totalHours + ' hrs' }}
                        </div>
                    </div>
                </shiftbreak>
            </div>
            <div class="row">
                <div class="columns small-12">
                    <hr />
                    <h4 class="reveal__content--tight">Day Notes</h4>
                    <p style="padding:6px 0 12px">{{ dayNotes }}</p>
                </div>
            </div>
            <div class="row">
                <div class="columns small-12">
                    <h4 class="reveal__content--tight">Shift Notes</h4>
                    <p style="padding:6px 0 12px">{{ shiftNotes }}</p>
                </div>
            </div>
            <div v-show="isCommentThreadShown" class="row">
                <div class="columns small-12">
                    <ul class="accordion" data-accordion>
                        <li id="liDropRequestComments" class="accordion-navigation">
                            <a href="#dropRequestCommentsAccordion" class="light" data-view-comments>
                                Comments
                                <span data-bind="text: '(' + comments().length + ')'"></span>
                                <div class="right icon plus" data-add-comment></div>
                            </a>
                            <div id="dropRequestCommentsAccordion" class="content">
                                <div class="comment-list">
                                    <textarea v-model="newDropRequestComment" :maxlength="commentsMaxCharCount" data-new-comment></textarea>
                                    <charactercount :text-value="newDropRequestComment" :max-character-count="commentsMaxCharCount" inline-template>
                                        <div class="ut-flex-end" data-new-comment-count>
                                            <span class="comment-list__character-count">{{ charactersRemainingMessage }}</span>
                                        </div>
                                    </charactercount>
                                    <div data-bind="foreach: comments()">
                                        <div class="comment-list__comment">
                                            <div class="comment__author" data-bind="text: modifiedBy"></div>
                                            <div class="comment__timestamp" data-bind="text: $root.printDateTime(localizedCreatedOn)"></div>
                                            <div class="comment__text" data-bind="text: comment"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="reveal__footer">
            
            <template v-if="isActioningPendingDropAllowed">
                <button v-if="areNewCommentsAllowed" :disabled="isSavingComments" type="button" class="button secondary" v-on:click="closeForm">Cancel</button>
                <button type="button" :disabled="isSavingComments" class="button secondary" v-on:click="onWithdrawDropShiftClick">Withdraw Coverage Request</button>
            </template>
            <button v-else-if="areNewDropRequestsAllowed" type="button" :disabled="isShiftTimesheetLinked" class="button secondary" v-on:click="onDropShiftClick">Request Coverage</button>
            
            <button v-if="areNewCommentsAllowed" :disabled="isSavingComments" type="button" class="button" v-on:click="onSave">Save</button>
            <button v-else type="button" class="button" v-on:click="closeForm">Close</button>
        </div>
    </div>
    <div id="dropShiftWithdrawModalscheduledShiftForm" ref="dropShiftWithdrawModalscheduledShiftForm" class="reveal-modal small" data-reveal>
        <div class="reveal__header">
            <h2>Withdraw Coverage Request</h2>
            <a class="reveal__close" data-ga="controls-close form" v-on:click="closeDropShiftWithdrawModal"></a>
        </div>
        <div class="reveal__body--scrollable" :class="{ 'ut-loading withdraw-modal__body--loading': isSavingRequest }">
            <div class="row">
                <div class="columns small-12 reveal__content--tight">
                    <div class="alert-box warning">
                        Withdrawing your request for coverage will make this shift unavailable for other employees to request to work. You will remain assigned and are responsible to work this shift.
                    </div>
                </div>
            </div>
        </div>
        <div class="reveal__footer">
            <button type="button" :disabled="isSavingRequest" class="button secondary" v-on:click="closeDropShiftWithdrawModal">Close</button>
            <button type="button" :disabled="isSavingRequest" class="button" v-on:click="onWithdrawDropShiftConfirmed">Withdraw and Save</button>
        </div>
    </div>
    <div id="dropShiftModalscheduledShiftForm" ref="dropShiftModalscheduledShiftForm" class="reveal-modal small" data-reveal>
        <div class="reveal__header">
            <h2>Request Coverage</h2>
            <a class="reveal__close" data-ga="controls-close form" v-on:click="closeDropShiftModal"></a>
        </div>
        <div class="reveal__body--scrollable" :class="{ 'ut-loading': isSavingRequest }">
            <div class="row">
                <div v-show="isShiftWithinDropThreshold">
                    <div class="columns small-12">
                        <div class="alert-box warning">
                            {{ shiftWithinDropThresholdWarning }}
                        </div>
                    </div>
                </div>
                <div v-show="!isShiftWithinDropThreshold">
                    <div class="columns small-12 reveal__content--tight">
                        <label class="input-label--required">Reason</label>
                        <p>Include a reason for why you are requesting coverage of your shift. This note will only be visible to your manager.</p>
                    </div>
                    <div class="columns small-12 reveal__content--tight">
                        <textarea
                            ref="dropShiftModalReasonscheduledShiftForm"
                            :class="{ 'input--error': isDropShiftReasonInvalid }"
                            v-model="dropShiftReason"
                            :maxlength="commentsMaxCharCount"></textarea>
                        <span v-if="isDropShiftReasonInvalid" class="input__error-msg">Required</span>
                        <charactercount v-else :text-value="dropShiftReason" :max-character-count="commentsMaxCharCount" inline-template>
                            <div class="ut-flex-end">
                                <span class="comment-list__character-count">{{ charactersRemainingMessage }}</span>
                            </div>
                        </charactercount>
                    </div>
                    <div class="columns small-12">
                        <div class="alert-box info">
                            You are still assigned to this shift. It is your responsibility to work this shift until it has been reassigned, or removed from your schedule.
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="reveal__footer">
            <div v-if="isShiftWithinDropThreshold">
                <button
                    type="button"
                    class="button"
                    v-on:click="closeDropShiftModal">
                    Close
                </button>
            </div>
            <div v-else>
                <button
                    type="button"
                    :disabled="isSavingRequest"
                    class="button secondary"
                    v-on:click="closeDropShiftModal">
                    Cancel
                </button>
                <button
                    type="button"
                    class="button"
                    :disabled="isDropShiftReasonInvalid || isSavingRequest"
                    v-on:click="onDropShiftConfirmed">
                    Request and Save
                </button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    /**
     * Vue component for keeping track of character count
     */
    var characterCountComponent = {
        props: ['maxCharacterCount', 'textValue'],
        
        computed: {
            charactersRemainingMessage: function() {
                if (this.remainingCharacterCount > 1) {
                    return `${this.remainingCharacterCount} Characters`;
                }
                if (this.remainingCharacterCount === 1) {
                    return `${this.remainingCharacterCount} Character`;
                }
                return 'Maximum Characters Reached';
            },

            remainingCharacterCount: function() {
                const currentLength = this.textValue ? this.textValue.length : 0;
                return this.maxCharacterCount - currentLength;
            },
        }
    };

    /**
     * Vue component for the scheduled shift breaks
     */
    var shiftBreakComponent = {
        props: ['shiftbreak'],

        computed: {
            breakIcon: function() {
                return {
                    'break-paid': this.shiftbreak.isPaid,
                    'break-unpaid': !this.shiftbreak.isPaid,
                };
            },

            formattedBreakRange: function() {
                return pw.times.getLongFormattedRange(this.shiftbreak.startTime, this.shiftbreak.endTime);
            },

            totalHours: function() {
                var totalHours = moment(this.shiftbreak.endTime).diff(this.shiftbreak.startTime, 'hours', true);
                return pw.numbers.roundAndFormat(totalHours, 2);
            }
        },
    };

    /**
     * Vue model for the scheduled shift form
     */
    const clientId = 'scheduledShiftForm';
    var shiftFormVue = new Vue({
        el: `#scheduleShiftForm${clientId}`,

        components: {
            'charactercount': characterCountComponent,
            'shiftbreak': shiftBreakComponent
        },

        data: {
            addRequestsByShiftId: {},
            breaks: [],
            commentsMaxCharCount: 750,
            commentThread: null,
            commentThreadChanges: null,
            dropRequestByShiftId: {},
            dropShiftReason: '',
            dropShiftReasonChanges: null,
            hasCompanyAvailableShiftsEnabled: false,
            hasDropRequestComments: false,
            isDropShiftReasonInvalid: false,
            isLoadingBreaks: false,
            isLoadingComments: false,
            isSavingComments: false,
            isSavingDrop: false,
            isSavingWithdraw: false,
            newDropRequestComment: '',
            schedulesWithDropShiftsById: {},
            shift: {},
            shiftStartTime: null
        },

        computed: {
            addShiftRequestCount: function() {
                return this.addRequestsByShiftId[this.shift.etmShiftId]?.length || 0;
            },

            areNewCommentsAllowed: function() {
                return this.isActioningPendingDropAllowed && this.isCommentThreadShown;
            },

            areNewDropRequestsAllowed: function() {
                return !this.isShiftInThePast && this.isDropShiftsEnabled;
            },

            dayNotes: function() {
                // Display a generic message if there are no day notes
                if(this.shift.dayNotes === '') {
                    return 'There are no day notes to display.';
                } else {
                    return this.shift.dayNotes;
                }
            },

            formattedEventDate: function() {
                return pw.dates.formatFullDateWithYear(this.shift.eventDate);
            },

            formattedShiftRange: function() {
                const format = pw.times.getTimeFormat(false);
                const emDash = htmlDecode(pw.ui.emDash);
                const formattedStartTime = moment(this.shift.startTime).format(format);

                switch (this.shift.endTimeDisplayOptionId) {
                    case lib.scheduledShifts.endTimeDisplayOptions.HIDE_TIME:
                        return 'Start at' + ' ' + formattedStartTime;
                    case lib.scheduledShifts.endTimeDisplayOptions.REPLACE_WITH_CLOSE:
                        return formattedStartTime + ' ' + emDash + ' ' + 'Close';
                    default:
                        return pw.times.getLongFormattedRange(this.shift.startTime, this.shift.endTime);
                }
            },

            hasScheduleDropShiftsEnabled: function() {
                return !!this.schedulesWithDropShiftsById[this.shift.etmScheduleId];
            },

            isActioningPendingDropAllowed: function() {
                return !this.isShiftInThePast && this.shiftHasDropRequest;
            },

            isCommentThreadShown: function() {
                return this.hasDropRequestComments;
            },

            isDropShiftsEnabled: function() {
                return this.hasCompanyAvailableShiftsEnabled && this.hasScheduleDropShiftsEnabled;
            },


            isSavingRequest: function() {
                return this.isSavingDrop || this.isSavingWithdraw;
            },

            isShiftDetailsLoading: function() {
                return this.isLoadingBreaks || this.isLoadingComments || this.isSavingComments;
            },

            isShiftInThePast: function() {
                return this.shiftStartTime?.isBefore(moment());
            },

            isShiftTimesheetLinked: function() {
                return this.shift.isTimesheetLinked;
            },

            isShiftWithinDropThreshold: function() {
                return this.scheduleDropThresholdMinutes && this.shiftStartTime?.diff(moment(), 'minutes', true) <= this.scheduleDropThresholdMinutes;
            },

            scheduleDropThresholdMinutes: function() {
                return this.schedulesWithDropShiftsById[this.shift.etmScheduleId]?.dropThresholdMinutes || 0;
            },

            shiftHasDropRequest: function() {
                return !!this.dropRequestByShiftId[this.shift.etmShiftId];
            },

            shiftNotes: function() {
                // Display a generic message if there are no shift notes
                if(this.shift.notes === '') {
                    return 'There are no shift notes to display.';
                } else {
                    return this.shift.notes;
                }
            },

            shiftStartDate: function() {
                return this.shiftStartTime ? this.shiftStartTime.clone().startOf('day') : null;
            },

            shiftWithinDropThresholdWarning: function() {
                const MINUTES_PER_HOUR = 60;
                const warningTemplate = 'You cannot request coverage for this shift. Requests for coverage must be submitted at least [[ThresholdHours]] hours before the start of your shift. If you cannot work this shift, please contact your admin.';
                const scheduleDropThresholdHours = Math.floor(this.scheduleDropThresholdMinutes / MINUTES_PER_HOUR);

                return warningTemplate.replace("[[ThresholdHours]]", scheduleDropThresholdHours);
            }
        },

        watch: {
            dropShiftReason: function() {
                this.isDropShiftReasonInvalid = false;
            },
            shift: function(newShift, oldShift) {
                var vm = this;

                // Retrieve the breaks for the new shift
                this.isLoadingBreaks = true;
                $.get('/pwnextv2api/etm/scheduledshiftbreaks?id=' + newShift.id).done(function(breaks) {
                    vm.breaks = breaks;
                    vm.isLoadingBreaks = false;
                });
            }
        },

        methods: {
            closeDropShiftModal: function() {
                return this.dropShiftReasonChanges.discard().then(() => {
                    return $(this.$refs[`dropShiftModal${clientId}`]).pwRevealModal('hide');
                });
            },

            closeDropShiftWithdrawModal: function() {
                return $(this.$refs[`dropShiftWithdrawModal${clientId}`]).pwRevealModal('hide');
            },

            /** Close the scheduled shift form */
            closeForm: function() {
                var deferredCloseForm = $.Deferred();

                this.commentThreadChanges.discard().done(() => {
                    $(this.$refs[clientId]).pwRevealModal('hide').done(() => deferredCloseForm.resolve());
                });

                return deferredCloseForm;
            },

            expandCommentThread: function() {
                const isCommentThreadExpanded = this.commentThread.find('#dropRequestCommentsAccordion').hasClass('active');

                if (!isCommentThreadExpanded) {
                    this.commentThread.find('[href="#dropRequestCommentsAccordion"]').click();
                }
            },

            getDropRequestComments: function(shift) {
                const params = {
                    employeeShiftId: shift.etmShiftId,
                    employeeShiftRequestTypeIds: [lib.scheduledShifts.employeeShiftRequestTypes.DROP]
                };

                return $.ajax({
                    url: '/pwnextv2api/v3.0/TimeManagement/EmployeeShiftRequestComments',
                    contentType: 'application/json; charset=utf-8',
                    data: $.param(params, true)
                });
            },

            hideCalendarPendingDropIcon: function() {
                this.shift.secondaryIcon = false;
                $(this.$refs[clientId]).trigger('ScheduledShiftVueForm:dropRequestUpdated', this.shiftStartDate);
            },

            /** Perform one-time setup operations */
            initialize(schedulesWithDropShiftsById, hasCompanyAvailableShiftsEnabled, dropRequestByShiftId, addRequestsByShiftId) {
                this.schedulesWithDropShiftsById = schedulesWithDropShiftsById;
                this.hasCompanyAvailableShiftsEnabled = hasCompanyAvailableShiftsEnabled;
                this.dropRequestByShiftId = dropRequestByShiftId;
                this.addRequestsByShiftId = addRequestsByShiftId;

                // Setup and initialize the comment thread and discard changes
                this.commentThread = $(this.$refs[clientId]).find('#liDropRequestComments').commentList();
                this.commentThreadChanges = $(this.$refs[clientId]).find('#liDropRequestComments').unsavedChanges();
                this.dropShiftReasonChanges = $(this.$refs[clientId]).find(`#dropShiftModal${clientId}`).unsavedChanges();
            },

            loadShiftStartTimeMomentJS: function() {
                /** 
                 * This datetime is being assigned to a data variable to avoid confusion that can occur when combining 
                 * computed variables with MomentJS. Particularly, certain MomentJS functions will modify datetime
                 * objects. When applied to the computed variables, it will actually alter the computed variable.
                 * This is often not expected since computed variables are supposed to be readonly (in most circumstances).
                 */
                this.shiftStartTime = moment(this.shift.startTime);
            },

            onDropShiftClick: function() {
                this.resetDropShiftModal();
                $(this.$refs[`dropShiftModal${clientId}`]).foundation('reveal', 'open');
            },

            onDropShiftConfirmed: function() {
                const reasonToSave = this.dropShiftReason.trim();

                if (reasonToSave.length === 0) {
                    this.isDropShiftReasonInvalid = true;
                    return;
                }
                this.isSavingDrop = true;

                this.submitDropRequest(this.shift, reasonToSave).done(() => {
                    this.dropShiftReasonChanges.setClean();
                    this.showCalendarPendingDropIcon();
                    this.closeDropShiftModal().done(() => {
                        this.isSavingDrop = false;
                        this.closeForm();
                    });
                }).fail(() => {
                    this.isSavingDrop = false;
                });
            },

            onSave: function() {
                this.isSavingComments = true;
                const dropRequest = this.dropRequestByShiftId[this.shift.etmShiftId];
                const commentToSave = this.prepareDropRequestCommentForSave();

                this.saveDropRequestComment(dropRequest, commentToSave).done(() => {
                    this.commentThreadChanges.setClean();
                    this.closeForm().done(() => {
                        this.isSavingComments = false;
                    });
                }).fail(() => {
                    this.isSavingComments = false;
                });
            },

            onWithdrawDropShiftClick: function() {
                $(this.$refs[`dropShiftWithdrawModal${clientId}`]).foundation('reveal', 'open');
            },

            onWithdrawDropShiftConfirmed: function() {
                this.isSavingWithdraw = true;
                const dropRequest = this.dropRequestByShiftId[this.shift.etmShiftId];
                const commentToSave = this.prepareDropRequestCommentForSave();

                this.withdrawDropRequest(dropRequest, commentToSave).done(() => {
                    this.commentThreadChanges.setClean();
                    this.hideCalendarPendingDropIcon();
                    this.closeDropShiftWithdrawModal().done(() => {
                        this.isSavingWithdraw = false;
                        this.closeForm();
                    });
                }).fail(() => {
                    this.isSavingWithdraw = false;
                });
            },

            /** Open the scheduled shift form */
            openForm: function(shift) {
                this.resetCommentThread();

                var element = $(this.$refs[clientId]).foundation('reveal', 'open');
                this.shift = shift;

                this.loadShiftStartTimeMomentJS();

                // The notes have now been seen if they were new
                if (this.shift.notesViewState === 'Unviewed') {
                    this.shift.notesViewState = 'Viewed';
                    this.saveScheduledShift(this.shift);
                    element.trigger('ScheduledShiftVueForm:notesViewed', this.shift);
                }

                // Fetch and process drop request comments for the shift
                
            },

            populateCommentThread: function(comments) {
                this.commentThread.populateCommentList(comments);
                this.hasDropRequestComments = comments.length > 0;
            },

            prepareDropRequestCommentForSave() {
                const comments = this.commentThread.serialize();
                return comments.length > 0 ? comments[0]["Comment"] : null
            },

            resetCommentThread: function() {
                this.newDropRequestComment = '';
                this.hasDropRequestComments = false;
                this.commentThread.populateCommentList([]);
            },

            resetDropShiftModal: function() {
                this.dropShiftReason = '';
                this.isDropShiftReasonInvalid = false;

                // Having to manually reset data-prev-val since it isn't being updated when dropShiftReason is cleared.
                // Without this unsavedChanges functionality doesn't reset properly.
                $(this.$refs[`dropShiftModalReason${clientId}`]).attr("data-prev-val", this.dropShiftReason);
            },

            saveDropRequestComment: function(dropRequest, comment) {
                const saveCommentDeferred = $.Deferred();

                if (comment) {
                    const postData = JSON.stringify([{ employeeShiftRequestId: dropRequest.id, comment }]);
                    retrievingCsrfToken.done(() => $.ajax({
                        type: 'POST',
                        url: '/pwnextv2api/v3.0/TimeManagement/EmployeeShiftRequestComments',
                        contentType: 'application/json; charset=utf-8',
                        headers: { 'RequestVerificationToken': apiTokenManager.csrfToken },
                        data: postData,
                        dataType: 'json'
                    }).done((ajaxResult) => {
                        saveCommentDeferred.resolve(ajaxResult);
                    }));
                } else {
                    saveCommentDeferred.resolve();
                }

                return saveCommentDeferred;
            },

            /** Saves the scheduled shift to the database */
            saveScheduledShift: function(shift) {
                const saveShiftDeferred = $.Deferred();
                const postData = JSON.stringify(shift);

                retrievingCsrfToken.done(() => $.ajax({
                    type: 'POST',
                    url: '/pwnextv2api/etm/ScheduledShifts',
                    contentType: 'application/json; charset=utf-8',
                    headers: { 'RequestVerificationToken': apiTokenManager.csrfToken },
                    data: postData,
                    dataType: 'json'
                }).done((ajaxResult) => {
                    saveShiftDeferred.resolve(ajaxResult);
                }));

                return saveShiftDeferred;
            },

            showCalendarPendingDropIcon: function() {
                this.shift.secondaryIcon = { class: 'status-pending' };
                $(this.$refs[clientId]).trigger('ScheduledShiftVueForm:dropRequestUpdated', this.shiftStartDate);
            },

            submitDropRequest: function(shift, reason) {
                const submitDropDeferred = $.Deferred();
                const postData = JSON.stringify({
                    employeeShiftId: shift.etmShiftId,
                    typeId: lib.scheduledShifts.employeeShiftRequestTypes.DROP,
                    comment: reason
                });

                retrievingCsrfToken.done(() => $.ajax({
                    type: 'POST',
                    url: '/pwnextv2api/v3.0/TimeManagement/EmployeeShiftRequests',
                    contentType: 'application/json; charset=utf-8',
                    headers: { 'RequestVerificationToken': apiTokenManager.csrfToken },
                    data: postData
                }).done((ajaxResult) => {
                    this.$set(this.dropRequestByShiftId, ajaxResult.employeeShiftId, ajaxResult);
                    submitDropDeferred.resolve(ajaxResult);
                }));

                return submitDropDeferred;
            },

            withdrawDropRequest: function(dropRequest, comment) {
                const withdrawDropDeferred = $.Deferred();
                const postData = JSON.stringify({
                    id: dropRequest.id,
                    rowVersion: dropRequest.rowVersion,
                    comment
                });

                retrievingCsrfToken.done(() => $.ajax({
                    type: 'PUT',
                    url: '/pwnextv2api/v3.0/TimeManagement/EmployeeShiftRequests/Withdraw',
                    contentType: 'application/json; charset=utf-8',
                    headers: { 'RequestVerificationToken': apiTokenManager.csrfToken },
                    data: postData
                }).done((ajaxResult) => {
                    this.dropRequestByShiftId[ajaxResult.employeeShiftId] = undefined;
                    this.addRequestsByShiftId[ajaxResult.employeeShiftId] = undefined;
                    withdrawDropDeferred.resolve(ajaxResult);
                }));

                return withdrawDropDeferred;
            }
        },
    });
</script>

