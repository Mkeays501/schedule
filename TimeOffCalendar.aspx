        function bindTorButtons(close, event) {
            var $button = $(event.target);
            var statusId = $button.data('new-status');
            var form = this;

            form.validate(statusId).done(function() {
                form.setStatus(statusId);
                form.save($button).done(function(data) { close(event, data.d); });
            });
        }

        /**
         * Retrieves and filters the events and then displays them on the calendar
         * @param {object} visibleRange - The date range used to retrieve events
         */
        function showVisibleCalendarEvents(event, visibleRange) {
            agenda.data([], {}, []);

            var params = {
                lowerBound: pw.dates.formatServer(visibleRange.lowerBound),
                // Up to but not including midnight of the next day
                upperBound: pw.dates.formatServer(visibleRange.upperBound.clone().add(1, 'days'))
            };

            // If shared calendar items are present but the user has turned them off, return data for the current user only
            if (true && true && !($('#chkSharedCalendar').is(':checked'))) {
                params.employeeId = 809;
            }

            var torDeferred = $.Deferred();
            var shiftDeferred = $.Deferred();
            var holidayDeferred = $.Deferred();

            availableTors = [];
            availableShifts = [];
            availableHolidays = {};

            // TORs
            getTorData(params).done(function(ajaxResult) {
                availableTors = lib.timeOffRequests.getCalendarEvents(ajaxResult, false, PayworksSessionData.use24Hour, PayworksSessionData.isFrench, 809);
            })
            .always(function() {
                torDeferred.resolve();
            });

            // Shifts
            pendingRequestsDeferred.done( () => {
                getScheduledShiftData(params).done(function(ajaxResult) {
                    availableShifts = lib.scheduledShifts.getCalendarEvents(ajaxResult, PayworksSessionData.use24Hour, PayworksSessionData.isFrench, 809,
                    {
                        localization: {
                            endTimeHideOptionText: 'Start at',
                            endTimeCloseOptionText: 'Close'
                        }
                    },
                    pendingDropRequestByShiftId);
                })
                .always(function() {
                    shiftDeferred.resolve();
                });
            });

            // Holidays
            getHolidayData(params).done(function(ajaxResult) {
                availableHolidays = lib.holidays.getCalendarEvents(ajaxResult);
            })
            .always(function() {
                holidayDeferred.resolve();
            });

            var deferred = calendar.working(
                agenda.working(
                    $.when(torDeferred, shiftDeferred, holidayDeferred).done(function() {
                        calendar.holidays(availableHolidays);
                        applyCalendarFilters (calendarFilter);
                        calendar.visibleDates(visibleRange);
                    })
                )
            );

            getDateRangeIndicators();

            return deferred;
        }

        function setVisibleEventTables(displayEventTypes) {
            // Find if there is access to the module and if the filter display event is checked or visible at all
            // Filter event will not be visible (and therefore displayEventTypes will be undefined) for Admin or when either AM or TM modules are not accessible
            var areFiltersUsed = (displayEventTypes !== undefined);
            var visibleTorSection = true && (!areFiltersUsed || $.inArray('tor', displayEventTypes) >= 0);
            var visibleShiftSection = true && (!areFiltersUsed || $.inArray('shift', displayEventTypes) >= 0);

            // Display/hide sections in the day modal depending if they are to be visible
            $('#dayModalTorTableSection').toggle(visibleTorSection);
            $('#dayModalShiftTableSection').toggle(visibleShiftSection);

            
            agenda.toggleTableViews(visibleTorSection, visibleShiftSection);
            
        }

        function torEventClicked(event, day, torEvent) {
            calendarDateClicked(event, day).done(function() {
                var row = calendarDateTorTable.getRequestRow(torEvent.groupId, torEvent.id);

                // Highlight the row without clicking on it
                if(torEvent.userIsOwner) row.addClass('selected');
                scrollToTableRow(row);
            });
        }

        function shiftEventClicked(event, day, shiftEvent) {
            calendarDateClicked(event, day).done(function() {
                var row = calendarDateShiftTable.row(function(ix, data, node) {
                    return data.id == shiftEvent.id;
                });

                if(shiftEvent.userIsOwner) row.select();
                scrollToTableRow($(row.node()));
            });
        }

        // scrolls the modal to the desired row so it displays in the middle if possible
        function scrollToTableRow(row) {
            var container = $('#calendarDateModal .reveal__body--scrollable');

            container.scrollTop(
                row.offset()?.top - container.offset()?.top + container.scrollTop() - (container.height() / 2)
            );
        }

        function calendarDateClicked(event, day) {
            var date = moment(day.date);
            var allGroupIds = getGroupIdsOnDate(date);
            var populating = populateDayModal(date, allGroupIds);

            $('#calendarDateTorTable').waitForAsyncData(populating);
            $('#calendarDateModal').foundation('reveal', 'open');

            return populating;
        }

        function getGroupIdsOnDate (date) {
            return $.pwUnique( $.map($.grep(filteredTors,
                function(tor) { return date.isSame(tor.eventDate, 'day'); }),
                function(tor) { return tor.groupId })
            );
        }

        function populateDayModal(date, allGroupIds) {
            var previousDate = getPreviousDay(date);
            var nextDate = getNextDay(date);

            $("#leftArrow, #rightArrow").unbind('click');

            populateScheduledShiftsTable(date);

            // Retrieve the TORs and populate the table
            return getDailyTimeOffRequestGroups(allGroupIds).done(function(data) {
                $('#hSelectedDate').text(date.format(pikadayOptions.format));
                calendarDateTorTable.populateTable(data);

                // Bind the previous and next arrows
                bindArrow($("#leftArrow"), previousDate);
                bindArrow($("#rightArrow"), nextDate);
            });

            function bindArrow($arrow, date) {
                if (date.isValid()) {
                    $arrow.removeClass('disabled').off('click').on('click', function() {
                        populateDayModal(date, getGroupIdsOnDate(date));
                    });
                } else {
                    $arrow.addClass('disabled').off('click');
                }
            }
        }

        function populateScheduledShiftsTable(date) {
            var dailyShifts = filteredShifts.filter(function(shift) {
                return date.isSame(moment(shift.startTime), 'day');
            });

            calendarDateShiftTable.clear().rows.add(dailyShifts).columns.adjust().draw();
        }

        function getNextDay(currentDate) {
            var filteredEvents = filteredShifts.concat(filteredTors);

            var futureDays = $.grep($.map(filteredEvents,
                function(t) { return moment(t.eventDate); }),
                function(d) { return d.isAfter(currentDate, 'days'); });
            return moment(Math.min.apply(null, futureDays));
        }

        function getPreviousDay(currentDate) {
            var filteredEvents = filteredShifts.concat(filteredTors);

            var pastDays = $.grep($.map(filteredEvents,
                function(t) { return moment(t.eventDate); }),
                function(d) { return d.isBefore(currentDate, 'days'); });
            return moment(Math.max.apply(null, pastDays));
        }

        function loadCalendarData(dateRanges) {
            return showVisibleCalendarEvents.apply(this, [new CustomEvent('init')].concat(dateRanges));
        }

        function toggleDateSelector(e) {
            var dateSelection = e.target.value;
            var showMonthSelector = dateSelection === '0';

            // Don't do anything if the currently selected option is clicked
            if(showMonthSelector != dateRangeSelector.is(':visible')) {
                dateRangeSelector.toggle(showMonthSelector);
                customRangeSelector.toggle(!showMonthSelector);
                $('#accSmallRangeSelector').toggleClass('hide-for-small', showMonthSelector);

                saveViewPreferences({CalendarDateSelector: dateSelection});

                if(showMonthSelector) {
                    loadCalendarData(dateRangeSelector.selectDate(moment()));
                } else {
                    customRangeSelector.selectDate(dateRangeSelector.getDateBounds().lowerBound);
                }
            }
        }

        function applyCalendarFilters (filter) {
            var hasFilters = $.fn.itemFilter.hasAnyFilters(filter.getFilters());
            var selectedFilters = filter.getSelected();
            var showCalendar = $('#rdoCalendar').prop('checked');

            filteredTors = $.fn.calendarFilter.filterEvents(availableTors, selectedFilters, isAdmin);
            filteredShifts = $.fn.calendarFilter.filterEvents(availableShifts, selectedFilters, isAdmin);

            setPageData(showCalendar);
            setVisibleEventTables(selectedFilters.DisplayEventTypes);

            $('[data-reveal-id="calendarFilter"]').toggleClass('filter', !hasFilters).toggleClass('filter--active', hasFilters);
        }

        function initSelectors() {
            var currDate = moment();
            loadCalendarData(dateRangeSelector.selectDate(currDate)).done(function() {
                $(window).trigger('TimeOffCalendar:initialized');
            });
            customRangeSelector.selectDate(currDate);
        }

        function selectCurrentDate() {
            var currDate = moment().startOf('day');

            if(customRangeSelector.is(':visible')) {
                loadCalendarData(customRangeSelector.selectDate(currDate)).done(function(){
                    if($(agenda.selector).is(':visible')) {
                        agenda.scrollToClosestRow(currDate);
                    }
                });
            }
            else {
                loadCalendarData(dateRangeSelector.selectDate(currDate));
            }
        }

        function populatePendingTorTable() {
            if(true) {
                getTorsRequiringReview().done(function(data) {
                    pendingTorTable.populateTable(data);
                    setPendingTorHeader(data);
                });
            }
        }

        function toggleDisplaySharedCalendarSetting(event) {
            saveViewPreferences({ DisplaySharedCalendar: $(event.target).is(':checked') }).done(() => {
                loadCalendarData(dateRangeSelector.getRange()).done(() => {
                    toggleEmployeeNameColumn(this.checked);
                });
            })
        }

        function setupDropShifts() {
            // Load available shifts company rule
            var ruleDeferred = checkForClaimRole('ScheduleAvailableShiftsEnabled').done(function(ajaxResult) {
                hasCompanyAvailableShiftsEnabled = ajaxResult;
            });

            // Load drop shift schedules indexed by id
            var schedulesDeferred = getDropShiftSchedules().done(function(ajaxResult) {
                schedulesWithDropShiftsById = ajaxResult.toObject('id');
            });

            // Load pending shift requests
            getPendingRequests().done(function(ajaxResult) {
                    // Group the requests based on type
                    var pendingAddRequests = ajaxResult.filter(request => request.typeId === lib.scheduledShifts.employeeShiftRequestTypes.ADD);
                    var pendingDropRequests = ajaxResult.filter(request => request.typeId === lib.scheduledShifts.employeeShiftRequestTypes.DROP);

                    // Index the requests by the shift's ETM id
                    pendingAddRequestsByShiftId = pendingAddRequests.toDictionary('employeeShiftId');
                    pendingDropRequestByShiftId = pendingDropRequests.toObject('employeeShiftId');
            })
            .always(function() {
                pendingRequestsDeferred.resolve();
            });

            // Get the shift form setup with drop shifts
            $.when(ruleDeferred, schedulesDeferred, pendingRequestsDeferred).done(function() {
                // Note: This will only be called when all requests are successful!
                shiftFormVue.initialize(
                    schedulesWithDropShiftsById,
                    hasCompanyAvailableShiftsEnabled,
                    pendingDropRequestByShiftId,
                    pendingAddRequestsByShiftId
                );
            });
        }

        calendar.initialized().done(initSelectors);

        populatePendingTorTable();

        
            shiftFormVue.initialize({}, false, {}, {});
            pendingRequestsDeferred.resolve();
        
    });

    /**
     * Hide or show the employee name columns on the day modal and agenda
     */
