                { data : null, visible : false}, // Employee Start Date
                { data : null, visible : false}, // Seniority Date
                { data : null, visible : false}  // Accrual
            ]
        };

        function renderTimeColumn(data, type) {
            var time = pw.datetimes.serverDateTimeToMoment(data);
