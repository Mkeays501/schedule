                <div class="columns small-12 medium-4">
                    <label>Department</label>
                    <p class="schedule-details__overview-tile-text">{{ shift.departmentName || '—' }}</p>
                </div>
                <div class="columns small-12 medium-4 end">
                    <label>Schedule Name</label>
                    <p class="schedule-details__overview-tile-text">{{ shift.scheduleName }}</p>
                </div>
                <div v-if="isActioningPendingDropAllowed" class="columns small-12 medium-4 end">
                    <label>Shift Request(s)</label>
                    <p class="schedule-details__overview-tile-text">{{ addShiftRequestCount }}</p>
                </div>
            </div>
            <div v-if="breaks.length > 0">
                <div class="row">
                    <div class="columns small-12">
                        <hr />
                        <h4 class="reveal__content--tight">Breaks</h4>
                    </div>
                </div>
                <shiftbreak v-for="(shiftbreak, index) in breaks" v-bind:key="index" v-bind:shiftbreak="shiftbreak" inline-template>
                    <div class="row reveal__content--tight">
                        <div class="columns small-2 hide-for-medium-up">
                            <span class="icon" :class="breakIcon"></span>
                        </div>
                        <div class="columns small-10 hide-for-medium-up">
                            <div class="row">
                                <div class="columns small-12">
